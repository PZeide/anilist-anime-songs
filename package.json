    "@gera2ld/plaid-rollup": "~2.5.0",
    "@types/tampermonkey": "^4.0.5",
    "del-cli": "^4.0.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "prettier": "^2.6.2",
    "rollup-plugin-userscript": "^0.1.1"
